<Window x:Class="harjoitustyo.NameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:harjoitustyo"
        mc:Ignorable="d"
        Title="NameWindow" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="txtName" HorizontalAlignment="Left" Height="22" Margin="73,54,0,0" TextWrapping="Wrap" Text="Insert Player Name" VerticalAlignment="Top" Width="151"/>
        <Button x:Name="btnStart" Content="Start" HorizontalAlignment="Left" Height="29" Margin="80,128,0,0" VerticalAlignment="Top" Width="128" Click="btnStart_Click"/>

    </Grid>
</Window>
